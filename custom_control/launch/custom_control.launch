<launch>
  
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find custom_control)/config/custom_control.yaml" command="load"/>
 
  <!--  load the controllers -->
  <node name="controller_unspawner" pkg="controller_manager" type="controller_manager" respawn="false"
    output="screen" args="kill scaled_pos_joint_traj_controller"/>

  <!-- <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="my_cartesian_force_controller"/>
   -->
  <!-- <node name="controller_starter" pkg="controller_manager" type="controller_manager" respawn="false" 
    output="screen" args="start my_cartesian_force_controller"/> -->

  <node name="controller_starter" pkg="controller_manager" type="controller_manager" respawn="false" 
    output="screen" args="spawn my_cartesian_force_controller"/>
  
<!-- <node name="joy_to_geometry" pkg="custom_control" output="screen" type="joy_to_geometry.py"/> -->

  <!-- convert joint states to TF transforms for rviz, etc -->
  <!-- <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen" /> -->
</launch>
